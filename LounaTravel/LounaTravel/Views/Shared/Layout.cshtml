    <script>
        $(function () {
            $('body').on('click', '#btn-dangky', function (e) {
                if (!validateRes(e)) {
                    return;
                };
                e.preventDefault;
                var Username = $("#usernameRegister");
                var Password = $("#PasswordRegister");
                var FirstName = $("#lastName");
                var LastName = $("#firstName");
                var tbl_User = {
                    Username: Username.val(),
                    Password: Password.val(),
                    FirstName: FirstName.val(),
                    LastName: LastName.val(),
                };
                $.ajax({
                    url: '/User/Create',
                    type: 'POST',
                    data: JSON.stringify(tbl_User),
                    contentType: 'application/json; charset=utf-8',
                    dataType: 'json',
                    success: function (response) {
                        if (response.status == true) {
                            alert(response.message);
                            $.ajax({
                                url: '/User/Login',
                                type: 'POST',
                                data: {
                                    userName: Username.val(),
                                    password: Password.val(),
                                },
                                //dataType: "json",
                                success: function (result) {

                                    if (result.status == true) {
                                        if (window.location.href.search("/Tour/Details/") != -1) {
                                            window.location.reload();
                                        } else {
                                            $("#message").hide();
                                            $("#dangnhap").modal("hide");
                                            setTimeout(function () {
                                                $("#nav-Login").html(result.NavLoginSuccess);
                                                $("#content-login").html(result.ContentLoginSuccess);
                                            }, 500);
                                        }
                                    } else {
                                        $("#message").show().text("Tên tài khoản hoặc mật khẩu không đúng.");
                                    }

                                },
                                error: function () {
                                    alert('Error loading profile data');
                                }
                            })
                            Username.val("");
                            Password.val("");
                            FirstName.val("");
                            LastName.val("");
                            $("#RePasswordChange").val("");
                            $("#dangky").modal("toggle");
                        } else {
                            alert(response.message);
                        }
                    },
                    error: function (response) {
                        alert(response.message);
                    }
                })
                //setTimeout(function () {
                //    var originalModal = $('#dangky').clone();
                //    $('#dangky').remove();
                //    var myClone = originalModal.clone();
                //    $('#content-login').append(myClone);
                //}, 1000);

            });
        });

    </script>


