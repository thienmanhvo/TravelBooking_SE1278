    <script>
        //show nut dang nhap va dang ki
        function showUserNav() {
            document.getElementById("user-nav").classList.toggle("show-nav");
        }
        $(function () {
            $(window).on("scroll", function (e) {
                var usernav = $('#user-nav');
                var link = $('#bg-image').height() - 400;

                if (window.pageYOffset >= link) {
                    usernav.addClass("black-color");
                } else {
                    usernav.removeClass("black-color");
                }
            });
        });
        function ValidationUpdate() {

            var firstName = document.getElementById("firstNameEdit");
            var lastName = document.getElementById("lastNameEdit");
            var email = document.getElementById("EmailEdit");
            var phoneNumber = document.getElementById("telEdit");
            if (firstName.value == "") {
                alert('Bạn cần nhập first name');
                return false;
            }
            if (lastName.value == "") {
                alert('Bạn cần nhập lastname');
                return false;
            }
            if (email.value == "") {
                alert('Bạn cần nhập email');
                return false;
            } if (phoneNumber.value == "") {
                alert('Bạn cần nhập Phone Number');
                return false;
            }
            else {
                return true;
            }
        }
    </script>
